{
  "name": "tt-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.17.0",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.4.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-store": "0.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "react-scripts build",
    "build:dev": "cross-env REACT_APP_ENV=development npm-run-all build-css build-js",
    "build:int": "cross-env REACT_APP_ENV=integration npm-run-all build-css build-js",
    "build:prd": "cross-env REACT_APP_ENV=production npm-run-all build-css build-js",
    "build:stg": "cross-env REACT_APP_ENV=staging npm-run-all build-css build-js",
    "build:tst": "cross-env REACT_APP_ENV=test npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "lintj": "./node_modules/.bin/eslint --ext .js src/",
    "lints": "./node_modules/.bin/stylelint \"src/**/*.scss\"",
    "start": "cross-env REACT_APP_ENV=local npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "cross-env REACT_APP_ENV=unitTest CI=true react-scripts test --env=jsdom --coverage --colors",
    "test:local": "cross-env REACT_APP_ENV=unitTest react-scripts test --env=jsdom --coverage",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
